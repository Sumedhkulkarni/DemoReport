<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.demoreport">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".store" />
        <activity android:name=".store$Store_bar"/>
        <activity android:name=".store$Store_line"/>
        <activity android:name=".store$Store_pie"/>
        <activity android:name=".category" />
        <activity android:name=".staff" />
        <activity android:name=".time" />
        <activity android:name=".payment" />
        <activity android:name=".login" />
        <activity android:name=".Category_bar" />
        <activity android:name=".Category_line" />
        <activity android:name=".Category_pie"></activity>

    </application>

</manifest>